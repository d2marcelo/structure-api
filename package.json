{
  "name": "structure-core",
  "version": "0.0.1-alpha-4",
  "description": "Build data structures",
  "main": "dist/index.js",
  "scripts": {
    "compile": "node_modules/.bin/babel src -d dist -D -s",
    "prepublish": "npm run compile",
    "start": "node dist/start.js",
    "start-reqlite": "node_modules/forever/bin/forever start node_modules/reqlite/lib/node.js --port-offset 1 -s",
    "stop-reqlite": "node_modules/forever/bin/forever stop node_modules/reqlite/lib/node.js --port-offset 1 -s",
    "test-clean": "export NODE_ENV=test; node ./test/helpers/db.babel.js --drop --integration && gulp test-integration",
    "test-integration": "export NODE_ENV=test; node ./test/helpers/db.babel.js --integration && gulp mocha-integration",
    "test-unit": "export NODE_ENV=test; npm run start-reqlite; node ./test/helpers/db.babel.js --unit; gulp mocha-unit; npm run stop-reqlite"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/structurejs/core.git"
  },
  "keywords": [
    "data",
    "structure",
    "build"
  ],
  "author": "Chris Abrams <mail@chrisabrams.com>",
  "license": "TBD",
  "bugs": {
    "url": "https://github.com/structurejs/core/issues"
  },
  "homepage": "https://github.com/structurejs/core",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bcrypt": "^0.8.6",
    "body-parser": "^1.15.0",
    "chalk": "^1.1.3",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "eisley": "*",
    "express": "^4.13.3",
    "jsonwebtoken": "^5.7.0",
    "multer": "^1.1.0",
    "node-uuid": "^1.4.7",
    "request": "^2.72.0",
    "rethinkdb": "^2.3.2",
    "rethinkdbdash": "^2.3.17",
    "serve-static": "^1.10.2",
    "shorthash": "0.0.2",
    "structure-storage-disk-adaptor": "*",
    "superagent": "^2.0.0-alpha.3",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chance": "^0.8.0",
    "chunk-manifest-webpack-plugin": "0.0.1",
    "eslint": "^3.1.1",
    "file-loader": "^0.8.5",
    "forever": "^0.15.2",
    "gulp": "^3.9.0",
    "gulp-ava": "^0.11.0",
    "gulp-clean": "^0.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^2.0.0",
    "gulp-livereload": "^3.8.1",
    "gulp-mocha": "^2.2.0",
    "gulp-size": "^2.0.0",
    "gulp-util": "^3.0.7",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "node-libs-browser": "^0.5.3",
    "nodemon": "^1.8.1",
    "reqlite": "^2.3.0",
    "run-sequence": "^1.2.2",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "structure-storage-disk-adaptor": "0.0.1",
    "structure-test-helpers": "*",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.1.0",
    "url-loader": "^0.5.7"
  }
}
